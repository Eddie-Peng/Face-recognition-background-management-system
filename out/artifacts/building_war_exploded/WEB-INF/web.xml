<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>

    <!--dormServlet-->
    <servlet>
        <servlet-name>dormServlet</servlet-name>
        <servlet-class>com.servlet.dormServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dormServlet</servlet-name>
        <url-pattern>/dormServlet</url-pattern>
    </servlet-mapping>

    <!--FaceInsert-->
    <servlet>
        <servlet-name>FaceInsert</servlet-name>
        <servlet-class>controller_Servlet.FaceInsert</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FaceInsert</servlet-name>
        <url-pattern>/FaceInsert</url-pattern>
    </servlet-mapping>

    <!--FaceDelet-->
    <servlet>
        <servlet-name>FaceDelet</servlet-name>
        <servlet-class>controller_Servlet.FaceDelete</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FaceDelet</servlet-name>
        <url-pattern>/FaceDelet</url-pattern>
    </servlet-mapping>

    <!--InsertServletManager-->
    <servlet>
        <servlet-name>InsertServletManager</servlet-name>
        <servlet-class>controller_Servlet.InsertServletManager</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InsertServletManager</servlet-name>
        <url-pattern>/InsertServletManager</url-pattern>
    </servlet-mapping>

    <!--LoginServletManager-->
    <servlet>
        <servlet-name>LoginServletManager</servlet-name>
        <servlet-class>controller_Servlet.LoginServletManager</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServletManager</servlet-name>
        <url-pattern>/LoginServletManager</url-pattern>
    </servlet-mapping>
</web-app>